/**
 * data.$idx = idx를 반영한다.
 * element에 Data를 "list_item_value" instance명으로 주입한다.
 * @function
 * @param {HTMLElement} t HTMLElement
 * @param {Array|Object|*} data
 * @param {uint} idx
 */
window.b2link.element.injectValueForListItemAndIDX = function( t, data, idx )
{
	//////////window.TtwLog.timeStamp( "---- [ S ] - window.b2link.element.injectValueForListItemAndIDX():void----------" );
	data.$idx = idx;
	t.list_item_value = data;
	//////////window.TtwLog.timeStamp( "---- [ E ] - window.b2link.element.injectValueForListItemAndIDX():void----------" );
};

