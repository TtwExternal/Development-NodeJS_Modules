/**
 * LIST형 Component에서 Data 변경 시에 발생되는 이벤트
 * 사용시 문의 및 협의 바람(송선우)
 * @function
 * @param {HTMLElement} el
 * @param {window.b2link.eventDetail.LIST_DATA_CHANGED} evtDetail
 */
window.b2link.event.dispatchCE__LIST_DATA_CHANGED = function( el, evtDetail )
{
	//////////window.TtwLog.timeStamp( "---- [ S ] - window.b2link.event.dispatchCE__LIST_DATA_CHANGED():void----------" );

	window.b2link.event.dispatchCustomEvent( el, window.b2link.event.LIST_DATA_CHANGED, evtDetail );

	//////////window.TtwLog.timeStamp( "---- [ E ] - window.b2link.event.dispatchCE__LIST_DATA_CHANGED():void----------" );
};

