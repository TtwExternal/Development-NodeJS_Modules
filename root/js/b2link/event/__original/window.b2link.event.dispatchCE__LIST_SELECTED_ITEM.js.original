 * @param {window.b2link.eventDetail.LIST_SELECTED_ITEM} evtDetail
 */
window.b2link.event.dispatchCE__LIST_SELECTED_ITEM = function( el, evtDetail )
{
	//////////window.TtwLog.timeStamp( "---- [ S ] - window.b2link.event.dispatchCE__LIST_SELECTED_ITEM():void----------" );

	window.b2link.event.dispatchCustomEvent( el, window.b2link.event.LIST_SELECTED_ITEM, evtDetail );

	//////////window.TtwLog.timeStamp( "---- [ E ] - window.b2link.event.dispatchCE__LIST_SELECTED_ITEM():void----------" );
};

