/**
 * Navigation의 Item을 선택 하였을 시 발생되는 이벤트
 * @function
 * @param {HTMLElement} el
 * @param {window.b2link.eventDetail.NAVIGATION_ITEM_SELECTED} evtDetail
 */
window.b2link.event.dispatchCE__NAVIGATION_ITEM_SELECTED = function( el, evtDetail )
{
	//////////window.TtwLog.timeStamp( "---- [ S ] - window.b2link.event.dispatchCE__NAVIGATION_ITEM_SELECTED():void----------" );

	window.b2link.event.dispatchCustomEvent( el, window.b2link.event.NAVIGATION_ITEM_SELECTED, evtDetail );

	//////////window.TtwLog.timeStamp( "---- [ E ] - window.b2link.event.dispatchCE__NAVIGATION_ITEM_SELECTED():void----------" );
};

