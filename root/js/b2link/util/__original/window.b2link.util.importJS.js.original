/**
 * *.js를 불러와서 실행시킨다.
 * js를 작성하는 규칙이 존재한다. js상에서는 js url을 Parameter로 받게 된다.
 * @function
 * @param {String} url
 * @return {*}
 */
window.b2link.util.importJS = function( url )
{
	//window.TtwLog.timeStamp( "---- [ S ] - window.b2link.util.importJS():{*}----------" );

	var r = SUtilTemplateHTML.addJS__URL( url, arguments );
		//console.log( r );

	//window.TtwLog.timeStamp( "---- [ E ] - window.b2link.util.importJS():{*}----------" );

	return r;
};

