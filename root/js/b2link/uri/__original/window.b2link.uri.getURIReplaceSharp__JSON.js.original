/**
 * @function
 * @param {Object} data
 * @return {String}
 * #을 !MzU=!로 변경
 * &을 !Mzg=!로 변경
 */
window.b2link.uri.getURIReplaceSharp__JSON = function( data )
{
	//window.TtwLog.timeStamp( "---- [ S ] - window.b2link.uri.getURIReplaceSharp__JSON():{String}----------" );

	//window.TtwLog.timeStamp( "---- [ E ] - window.b2link.uri.getURIReplaceSharp__JSON():{String}----------return s;" );
	return JSON.stringify( data ).replace( /#/gi, "!MzU=!" ).replace( /&/gi, "!Mzg=!" );
};

